@model Wedding
<Partial name = "_NavBarPartial"/>
<h1>Plan Your Wedding!</h1>
<form action="AddWeddingToDb" method = "Post">
    <div class="form-group">
        <label for="WedderOne">Wedder One</label>
        <input asp-for="WedderOne" class="form-control">
        <span asp-validation-for = "WedderOne" class = "text text-danger"></span>
    </div>
    <div class="form-group">
        <label for="WedderTwo">Wedder Two</label>
        <input asp-for="WedderTwo" class="form-control">
        <span asp-validation-for = "WedderTwo" class = "text text-danger"></span>
    </div>
    <div class="form-group">
        <label for="Date">Date</label>
        <input asp-for="Date" class="form-control">
        <span asp-validation-for = "Date" class = "text text-danger"></span>
    </div>
    <div class="form-group">
        <label for="Address">Address</label>
        <input asp-for="Address" class="form-control">
        <span asp-validation-for = "Address" class = "text text-danger"></span>
    </div>
    <button type="submit" value = "register" class="btn btn-primary">Create</button>
</form>